{
  "name": "study-resources-center",
  "version": "1.0.0",
  "description": "Educational platform with study resources and proxy capabilities",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:all": "node start-servers.js",
    "start:python": "cd python-proxy && python -m uvicorn main:app --host 0.0.0.0 --port 6078",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "sass": "sass scss/main.scss:public/styles.css",
    "sass:watch": "sass scss/main.scss:public/styles.css --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:browser": "jest --selectProjects browser",
    "test:node": "jest --selectProjects node",
    "test:python": "jest --testMatch='**/__tests__/python/**/*.test.js'",
    "test:python:auto": "node run-python-tests.js",
    "test:integration": "jest --testMatch='**/__tests__/integration/**/*.test.js'",
    "test:all": "jest --coverage",
    "test:ci": "jest --ci --coverage",
    "test:update": "jest --updateSnapshot"
  },
  "keywords": [
    "education",
    "proxy",
    "ultraviolet",
    "games"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.1.3",
    "@prisma/client": "^6.5.0",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "jsonwebtoken": "^9.0.2",
    "pm2": "^6.0.5",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.1",
    "@vitejs/plugin-react": "^4.4.0",
    "axios-mock-adapter": "^1.22.0",
    "cross-fetch": "^4.0.0",
    "dotenv": "^16.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^16.0.0",
    "jest-mock": "^29.7.0",
    "jest-transform-stub": "^2.0.0",
    "mock-socket": "^9.3.1",
    "msw": "^2.0.11",
    "nock": "^13.4.0",
    "node-mocks-http": "^1.13.0",
    "nodemon": "^3.1.9",
    "prisma": "^6.5.0",
    "sass": "^1.72.0",
    "socket.io-client": "^4.7.2",
    "socket.io-mock": "^1.3.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
